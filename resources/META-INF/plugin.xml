<idea-plugin>
  <id>org.idea.latex.javadoc.plugin.LatexJavadocPlugin</id>
  <name>Latex Javadoc</name>
  <version>1.0</version>
  <vendor email="msafonovmail@gmail.com" url="https://github.com/MikeSafonov/idea-latex-javadoc-plugin">Mike Safonov</vendor>


  <description><![CDATA[
      Enter short description for your plugin here.<br>
      <em>most HTML tags may be used</em>
    ]]></description>

  <change-notes><![CDATA[
      Add change notes here.<br>
      <em>most HTML tags may be used</em>
    ]]>
  </change-notes>

  <idea-version since-build="145.0"/>


  <application-components>
    <component>
      <implementation-class>org.idea.latex.javadoc.plugin.TempFileManager</implementation-class>
    </component>
  </application-components>
  <project-components>
    <component>
      <implementation-class>org.idea.latex.javadoc.plugin.ProjectConfiguration</implementation-class>
    </component>
  </project-components>

  <extensions defaultExtensionNs="com.intellij">
    <lang.documentationProvider language="JAVA" implementationClass="org.idea.latex.javadoc.plugin.LatexDocumentationProvider" order="first"/>
    <projectConfigurable instance="org.idea.latex.javadoc.plugin.LatexConfigurable"
                         id="org.idea.latex.doclet.plugin.LatexDocletPlugin.Config" displayName="Latex Doclet" groupId="tools"/>
  </extensions>


  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>